<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="SignMapper">
  
  <select id="selectId" parameterType="list" resultType="com.web.sbMain.model.SignDTO">
  	SELECT * FROM OSCUSER
  	<where>
  		ID in
  		<foreach item="n" collection="list" open="(" separator="," close=")">
  			#{n}
  		</foreach>
  	</where>
  </select>
  
</mapper>